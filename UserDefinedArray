#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter size: ";
    cin >> n;

    int arr[100]; 

    for (int i = 0; i < n; i++) {
        cout << "Enter value: ";
        cin >> arr[i];
    }

    cout << "Values: ";
    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    return 0;
}   
